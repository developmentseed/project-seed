{
  "name": "project-seed",
  "version": "4.0.0",
  "description": "Starter application by Development Seed",
  "repository": {
    "type": "git",
    "url": "https://github.com/developmentseed/project-seed.git"
  },
  "author": {
    "name": "Development Seed",
    "url": "https://developmentseed.org"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/developmentseed/project-seed/issues"
  },
  "homepage": "https://github.com/developmentseed/project-seed",
  "scripts": {
    "postinstall": "[ -f app/assets/scripts/config/local.js ] || echo \"'use strict';\n\nexport default {\n  environment: 'development'\n};\" > app/assets/scripts/config/local.js",
    "serve": "NODE_ENV=development gulp serve",
    "build": "NODE_ENV=production gulp",
    "stage": "NODE_ENV=staging gulp",
    "lint": "eslint app/assets/scripts/ --ext .js",
    "lint:css": "stylelint 'app/assets/styles/**/**' 'app/assets/scripts/**/*.js'",
    "test": "echo \"No tests\" && exit 0"
  },
  "engines": {
    "node": "14.x"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
          ],
          "plugins": [
            "babel-plugin-styled-components",
            "@babel/plugin-transform-spread",
            "@babel/plugin-proposal-object-rest-spread"
          ]
        }
      ],
      "require-globify",
      "envify"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/plugin-transform-spread": "^7.12.13",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "babel-eslint": "^10.1.0",
    "babelify": "^10.0.0",
    "browser-sync": "^2.26.14",
    "browserify": "^17.0.0",
    "collecticons-processor": "^4.1.0",
    "del": "^6.0.0",
    "envify": "^4.1.0",
    "errorify": "^0.3.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-inclusive-language": "^2.1.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "gulp-csso": "^4.0.1",
    "gulp-exit": "0.0.2",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-load-plugins": "^2.0.6",
    "gulp-plumber": "^1.2.1",
    "gulp-rev": "^9.0.0",
    "gulp-rev-rewrite": "^4.0.0",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-terser": "^2.0.1",
    "gulp-useref": "^5.0.0",
    "prettier": "^2.2.1",
    "require-globify": "^1.4.1",
    "stylelint": "^13.10.0",
    "stylelint-config-recommended": "^3.0.0",
    "surge": "^0.21.7",
    "through2": "^4.0.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^4.0.0"
  },
  "dependencies": {
    "gulp-replace": "^1.0.0",
    "lodash.defaultsdeep": "^4.6.1",
    "polished": "^4.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1"
  }
}
