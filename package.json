{
  "name": "project-seed",
  "description": "Starter application by Development Seed",
  "version": "8.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/developmentseed/project-seed.git"
  },
  "author": {
    "name": "Development Seed",
    "url": "https://developmentseed.org"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/developmentseed/project-seed/issues"
  },
  "homepage": "https://github.com/developmentseed/project-seed",
  "scripts": {
    "serve": "npm run clean && NODE_ENV=development vite --port 9000",
    "build": "npm run clean && NODE_ENV=production tsc -b && vite build",
    "stage": "npm run clean && NODE_ENV=staging tsc -b && vite build",
    "clean": "rm -rf dist node_modules/.vite",
    "lint": "npm run lint:scripts",
    "lint:scripts": "eslint app/",
    "ts-check": "npx tsc --noEmit --skipLibCheck",
    "test": "jest"
  },
  "engines": {
    "node": "20.x"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/babel__core": "^7",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/react-dom": "^19.0.4",
    "@types/react": "^19.0.10",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-jest": "^29.7.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-react": "^7.37.4",
    "eslint": "^9.21.0",
    "globals": "^15.15.0",
    "jest-environment-jsdom": "^29.7.0",
    "portscanner": "^2.2.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.24.1",
    "typescript": "~5.7.2",
    "vite": "^6.2.0"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.3.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "alias": {
    "$components": "~/src/components",
    "$styles": "~/src/styles",
    "$utils": "~/src/utils",
    "$hooks": "~/src/hooks",
    "$pages": "~/src/pages",
    "$test": "~/test"
  }
}